/var/log/cboxredirectd/cboxredirectd_http.log {
    daily
    rotate 365
    copytruncate
    delaycompress
    compress
    notifempty
    missingok
}

/var/log/cboxredirectd/cboxredirectd_app.log {
    daily
    rotate 365
    copytruncate
    delaycompress
    compress
    notifempty
    missingok
}
